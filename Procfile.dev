server: cargo run --bin rise -- backend server
project: cargo run --bin rise -- backend controller project

# Enable only if the chosen registry type is "ecr"
ecr: cargo run --bin rise -- backend controller ecr

# Enable when using Kubernetes runtime (comment out backend-deployment above)
k8s: cargo run --bin rise -- backend controller deployment-kubernetes

